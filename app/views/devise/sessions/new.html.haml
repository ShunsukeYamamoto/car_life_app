.form_menu
  %h2.form_title ログイン画面
  = form_for(resource, as: resource_name, url: session_path(resource_name),html: {class: "form_head"}) do |f|
    .field
      = f.email_field :email, autofocus: true, autocomplete: "email",placeholder: "Eメールアドレス",class: "form form_email"
    .field
      = f.password_field :password, autocomplete: "current-password",placeholder: "パスワード(6文字以上)",class: "form form_password"
    -# - if devise_mapping.rememberable?
    -#   .field
    -#     = f.check_box :remember_me
    -#     = f.label :remember_me
    .actions
      = f.submit "ログイン",class: "form_btn"
  -# = render "devise/shared/links"
  %h1 管理者
  %p email: admin@admin  psw: adminadmin
  %h1 ユーザー
  %p email: user@user psw: useruser
